{
    "name": "yacc",
    "displayName": "VSCode-YACC",
    "description": "Provides support for YACC files within Visual Studio Code",
    "version": "1.3.0",
    "publisher": "carlubian",
    "icon": "images/icon-YACC.png",
    "homepage": "https://github.com/carlubian/VSCode-YACC",
    "galleryBanner": {
        "color": "#007ACC",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/carlubian/VSCode-YACC.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yacc",
            "aliases": ["YACC", "yacc"],
            "extensions": [".y"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yacc",
            "scopeName": "source.yacc",
            "path": "./syntaxes/yacc.tmLanguage.json"
        }]
    }
}
